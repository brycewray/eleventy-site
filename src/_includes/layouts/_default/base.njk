{% set eleventyEnv = projEnv.environment %}
<!DOCTYPE html>
<html lang="en">
  {% include 'layouts/partials/head.njk' %}
  <body>
    {% include 'layouts/partials/header.njk' %}
    {{ content | safe }}
    {% include 'layouts/partials/footer.njk' %}
    <script src="/assets/js/lazyload_17-5-0.min.js"></script>
    {% if eleventyEnv == "production" %}
      <script src="/assets/js/{{ lazyloadhelper.lazyloadJS }}"></script>
    {% else %}
      <script src="/assets/js/lazyload-helper.js"></script>
    {% endif %}
  </body>
</html>
