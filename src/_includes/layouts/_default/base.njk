{% set eleventyEnv = projEnv.environment %}
{% set siteHost = siteparams.siteHost %}

<!DOCTYPE html>
<html class="dark:bg-black" lang="en">
  {% include 'layouts/partials/head.njk' %}
  <body class="dark:bg-black dark:text-gray-100">
    {% include 'layouts/partials/header.njk' %}
    {{ content | safe }}
    {% include 'layouts/partials/footer.njk' %}
    {% if eleventyEnv == "production" %}
      <script src="/assets/js/lazyload_17-5-0.min.js"{% if siteHost == "CFP" %} nonce="DhcnhD3khTMePgXw"{% endif %}></script>
      <script src="/assets/js/{{ lazyloadhelper.lazyloadJS }}"{% if siteHost == "CFP" %} nonce="DhcnhD3khTMePgXw"{% endif %}></script>
      {% if siteHost == "CFP" %}
        <!-- Cloudflare Web Analytics -->
        <script defer src='https://static.cloudflareinsights.com/beacon.min.js' data-cf-beacon='{"token": "36afbbd88c1b4b608a61683f9555fd48"}'>
        </script>
        <!-- End Cloudflare Web Analytics -->
      {% endif%}
    {% else %}
      <script src="/assets/js/lazyload_17-5-0.min.js"></script>
      <script src="/assets/js/lazyload-helper.js"></script>
    {% endif %}
  </body>
</html>
