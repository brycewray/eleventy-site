{
  "build": {},
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "interest-cohort=()"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; style-src 'self' 'unsafe-inline' https://ton.twimg.com platform.twitter.com; img-src 'self' https://*.brycewray.com https://*.cloudinary.com abs.twimg.com https://pbs.twimg.com ton.twimg.com platform.twitter.com https://syndication.twitter.com data:; font-src 'self'; frame-src https://twitter.com platform.twitter.com syndication.twitter.com; script-src 'self' 'unsafe-eval' https://*.brycewray.com https://cdn.jsdelivr.net https://cdn.syndication.twimg.com platform.twitter.com https://static.cloudflareinsights.com"
        }
      ]
    },
    {
      "source": "/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2678400, immutable"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "public, max-age=2678400, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2678400, immutable"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "public, max-age=2678400, immutable"
        }
      ]
    },
    {
      "source": "/images/icons/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2678400, immutable"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "public, max-age=2678400, immutable"
        }
      ]
    },
    {
      "source": "/assets/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2678400, immutable"
        }
      ]
    },
    {
      "source": "/assets/svg/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2678400, immutable"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "public, max-age=2678400, immutable"
        }
      ]
    },
    {
      "source": "/assets/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2678400, immutable"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "public, max-age=2678400, immutable"
        }
      ]
    }
  ]
}