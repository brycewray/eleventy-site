{
  "name": "eleventy_solo",
  "description": "",
  "private": true,
  "scripts": {
    "clean": "rm -rf _site",
    "sass": "sass --style=compressed --no-source-map src/assets/scss/index.scss src/_includes/css/index.css",
    "prefix": "postcss src/_includes/css/index.css --use=autoprefixer -o src/_includes/css/index.css",
    "start": "npm-run-all clean --parallel dev:*",
    "dev:bsync": "browser-sync start --server ./_site -w --no-open --no-notify --no-ghost-mode",
    "dev:sass": "npm run sass -- --watch",
    "dev:prefix": "npm run prefix --watch",
    "dev:eleventy": "ELEVENTY_ENV=development npx @11ty/eleventy --watch --quiet",
    "build": "NODE_ENV=production npm-run-all clean prod:sass prod:eleventy",
    "prod:sass": "npm run sass",
    "prod:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy --output=./_site",
    "testbuild": "NODE_ENV=production npm-run-all clean --parallel testProd:*",
    "testProd:bsync": "browser-sync start --server ./_site -w --no-open --no-notify --no-ghost-mode",
    "testProd:sass": "npm run sass -- --watch",
    "testProd:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy --output=./_site --watch"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "browserslist": [
    "last 4 versions",
    "> 1%",
    "IE 11",
    "maintained node versions"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/register": "^7.13.8",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "babel-core": "^6.26.3",
    "babel-plugin-prismjs": "^2.0.1",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.14",
    "cross-env": "^7.0.3",
    "debug": "^4.3.1",
    "del": "^6.0.0",
    "eleventy-plugin-error-overlay": "^0.1.2",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "eleventy-rss-helper": "^1.2.1",
    "fibers": "^5.0.0",
    "glob": "^7.1.6",
    "glob-all": "^3.2.1",
    "html-minifier": "^4.0.0",
    "inline-source-map": "^0.6.2",
    "lodash": "^4.17.21",
    "luxon": "^1.26.0",
    "markdown-it": "^12.0.4",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-link-attributes": "^3.0.0",
    "markdown-it-prism": "^2.1.6",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "posthtml": "^0.15.1",
    "prismjs": "^1.23.0",
    "rss": "^1.2.2",
    "sanitize-html": "^2.3.2",
    "sass": "^1.32.8"
  }
}
