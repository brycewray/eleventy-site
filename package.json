{
  "name": "eleventy_site",
  "description": "The repository for brycewray.com, a website built on the Eleventy (https://11ty.dev) static site generator. Built off the Git history of the earlier `eleventy_solo` repo.",
  "private": true,
  "scripts": {
    "cleanDev": "rimraf '_site/!(images)'",
    "cleanProd": "rimraf _site",
    "hasher": "node csshash.js && node jshash.js",
    "devsass": "sass --no-source-map src/assets/scss/index.scss _site/css/index.css",
    "prodsass": "sass --no-source-map src/assets/scss/index.scss _site/css/$(cat scsshash-out) --style=compressed",
    "start": "NODE_ENV=development npm-run-all cleanDev devsass --parallel dev:*",
    "build": "NODE_ENV=production npm-run-all cleanProd hasher prod:sass prod:eleventy",
    "dev:sass": "npm-run-all devsass -- --watch",
    "dev:eleventy": "ELEVENTY_ENV=development npx @11ty/eleventy --incremental --quiet --serve",
    "prod:sass": "npm-run-all prodsass",
    "prod:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy --output=./_site"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-canary.3",
    "@11ty/eleventy-fetch": "^3.0.0",
    "@11ty/eleventy-img": "^1.1.0",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "axios": "^0.26.1",
    "cross-env": "^7.0.3",
    "eleventy-plugin-embed-tweet": "^0.3.1",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "eleventy-rss-helper": "^1.2.1",
    "fast-glob": "^3.2.11",
    "glob": "^7.2.0",
    "html-minifier": "^4.0.0",
    "luxon": "^2.3.1",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-attrs": "^4.1.3",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-link-attributes": "^4.0.0",
    "markdown-it-prism": "^2.2.3",
    "md5": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rss": "^1.2.2",
    "sass": "^1.49.9"
  }
}
