{
  "name": "eleventy_site",
  "description": "The repository for brycewray.com, a website built on the Eleventy (https://11ty.dev) static site generator. Built off the Git history of the earlier `eleventy_solo` repo.",
  "private": true,
  "scripts": {
    "clean": "rimraf _site",
    "hasher": "node csshash.js && node jshash.js",
    "start": "TAILWIND_MODE=watch NODE_ENV=development npm-run-all clean --parallel dev:*",
    "dev:twcss": "tailwindcss --postcss -i src/assets/css/index.css -o _site/css/index.css --watch",
    "dev:eleventy": "ELEVENTY_ENV=development npx @11ty/eleventy --quiet --incremental --serve",
    "build": "NODE_ENV=production npm-run-all clean hasher --parallel prod:*",
    "prod:twcss": "tailwindcss --postcss -i src/assets/css/index.css -o _site/css/$(cat csshash-out) --minify",
    "prod:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy --output=./_site",
    "testbuild:bsync": "browser-sync start --server ./_site -w --no-open --no-notify --no-ghost-mode --port 3000 --ui-port 3001",
    "testbuild": "NODE_ENV=production npm run build && npm run testbuild:bsync"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0-beta.2",
    "@11ty/eleventy-img": "^1.0.0",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-upgrade-help": "^1.0.1",
    "cross-env": "^7.0.3",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "eleventy-rss-helper": "^1.2.1",
    "fast-glob": "^3.2.7",
    "glob": "^7.2.0",
    "html-minifier": "^4.0.0",
    "luxon": "^2.2.0",
    "markdown-it": "^12.3.0",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-attrs": "^4.1.0",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-link-attributes": "^3.0.0",
    "markdown-it-prism": "^2.2.1",
    "md5": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "rimraf": "^3.0.2",
    "rss": "^1.2.2",
    "tailwindcss": "^3.0.2",
    "tailwindcss-logical": "^2.0.0"
  }
}
