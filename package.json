{
  "name": "eleventy_site",
  "description": "The repository for www.brycewray.com, a website built on the Eleventy (https://11ty.dev) static site generator. Built off the Git history of the earlier `eleventy_solo` repo.",
  "private": true,
  "scripts": {
    "cleanDev": "rimraf '_site/!(images)'",
    "cleanProd": "rimraf _site",
    "search": "pagefind --source _site",
    "clean:css": "rimraf src/_includes/css && rimraf src/css && mkdirp src/_includes/css && mkdirp src/css",
    "style-exe:cssPtls": "postcss src/assets/cssfrom/partials --dir src/_includes/css",
    "style-exe:cssMain": "postcss src/assets/cssfrom/main --dir src/css",
    "style-dev:cssPtls": "npm run style-exe:cssPtls -- --watch",
    "style-dev:cssMain": "npm run style-exe:cssMain -- --watch",
    "style-dev:css": "npm-run-all --parallel style-dev:cssPtls style-dev:cssMain",
    "style-prod:css": "npm-run-all style-exe:cssPtls style-exe:cssMain",
    "start": "NODE_ENV=development npm-run-all cleanDev clean:css style-exe:cssPtls style-exe:cssMain --parallel style-dev:css dev:eleventy",
    "build": "NODE_ENV=production npm-run-all cleanProd clean:css style-exe:cssPtls style-exe:cssMain style-prod:css prod:eleventy search",
    "testbuild": "NODE_ENV=production npm-run-all cleanDev clean:css style-exe:cssPtls style-exe:cssMain --parallel style-dev:css testProd:eleventy",
    "dev:eleventy": "ELEVENTY_ENV=development npx @11ty/eleventy --serve --incremental",
    "prod:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy",
    "testProd:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy --serve"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-fetch": "^4.0.0",
    "@11ty/eleventy-img": "^3.0.0",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@11ty/eleventy-plugin-webc": "^0.10.0",
    "axios": "^1.3.4",
    "clean-css": "^5.3.2",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "eleventy-rss-helper": "^1.2.1",
    "fast-glob": "^3.2.12",
    "glob": "^9.3.2",
    "html-minifier": "^4.0.0",
    "luxon": "^3.3.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.3.0",
    "md5": "^2.3.0",
    "mkdirp": "^2.1.6",
    "node-fetch": "^3.3.1",
    "npm-run-all": "^4.1.5",
    "pagefind": "^0.12.0",
    "rimraf": "^4.4.1",
    "rss": "^1.2.2"
  }
}
