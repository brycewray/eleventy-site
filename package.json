{
  "name": "eleventy-site",
  "description": "A website built on the Eleventy (https://11ty.dev) static site generator. Built off the Git history of the earlier `eleventy_solo` repo.",
  "private": true,
  "scripts": {
    "cleanDev": "rimraf '_site/!(images)' && rimraf 'src/_includes/css' && rimraf '_data/csshash.json'",
    "cleanProd": "rimraf _site && rimraf 'src/_includes/css' && rimraf '_data/csshash.json'",
		"hasher": "node csshash.js",
    "search": "pagefind --source _site",
    "start": "NODE_ENV=development npm-run-all cleanDev hasher dev:eleventy",
    "build": "NODE_ENV=production npm-run-all cleanProd hasher prod:eleventy search",
    "testbuild": "NODE_ENV=production npm-run-all cleanDev hasher testProd:eleventy",
    "dev:eleventy": "ELEVENTY_ENV=development npx @11ty/eleventy --serve --incremental",
    "prod:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy",
    "testProd:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy --serve"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-fetch": "^4.0.0",
    "@11ty/eleventy-img": "^3.1.8",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "axios": "^1.6.5",
    "clean-css": "^5.3.3",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "date-fns": "^3.2.0",
    "dotenv": "^16.3.1",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "eleventy-rss-helper": "^1.2.1",
    "glob": "^10.3.10",
    "html-minifier": "^4.0.0",
    "luxon": "^3.4.4",
    "markdown-it": "^14.0.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.3.0",
    "md5": "^2.3.0",
    "mkdirp": "^3.0.1",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "pagefind": "^1.0.4",
    "rimraf": "^5.0.5",
    "rss": "^1.2.2"
  }
}
