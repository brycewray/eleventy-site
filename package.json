{
  "name": "eleventy_site",
  "description": "The repository for brycewray.com, a website built on the Eleventy (https://11ty.dev) static site generator. Built off the Git history of the earlier `eleventy_solo` repo.",
  "private": true,
  "scripts": {
    "clean": "rimraf _site",
    "hasher": "node csshash.js && node jshash.js",
    "devsassFontsPublicSans": "sass --no-source-map src/assets/scss/_fonts_PublicSans.scss _site/css/fonts_PublicSans.css",
    "devsassFontsInter": "sass --no-source-map src/assets/scss/_fonts_Inter.scss _site/css/fonts_Inter.css",
    "devsassInterVF": "sass --no-source-map src/assets/scss/_intervf.scss _site/css/intervf.css",
    "devsass": "sass --no-source-map src/assets/scss/index.scss _site/css/index.css",
    "prodsassFontsInter": "sass --no-source-map src/assets/scss/_fonts_Inter.scss _site/css/$(cat scssfonts_interhash-out) --style=compressed",
    "prodsassInterVF": "sass --no-source-map src/assets/scss/_intervf.scss _site/css/$(cat scssintervfhash-out) --style=compressed",
    "prodsassFontsPublicSans": "sass --no-source-map src/assets/scss/PublicSans.scss _site/css/$(cat scssfonts_publicsanshash-out) --style=compressed",
    "prodsass": "sass --no-source-map src/assets/scss/index.scss src/_includes/css/index.css --style=compressed",
    "start": "NODE_ENV=development npm-run-all clean devsass --parallel dev:*",
    "build": "NODE_ENV=production npm-run-all clean hasher prod:sass prod:eleventy",
    "dev:sass": "npm-run-all devsassFontsPublicSans devsass -- --watch",
    "dev:eleventy": "ELEVENTY_ENV=development npx @11ty/eleventy --incremental --quiet --serve",
    "prod:sass": "npm-run-all prodsassFontsPublicSans prodsassInterVF prodsass",
    "prod:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy --output=./_site",
    "testbuild:bsync": "browser-sync start --server ./_site -w --no-open --no-notify --no-ghost-mode --port 3000 --ui-port 3001",
    "testbuild": "NODE_ENV=production npm run build && npm run testbuild:bsync"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-fetch": "^3.0.0",
    "@11ty/eleventy-img": "^1.1.0",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "axios": "^0.26.0",
    "cross-env": "^7.0.3",
    "eleventy-plugin-embed-tweet": "^0.3.1",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "eleventy-rss-helper": "^1.2.1",
    "fast-glob": "^3.2.11",
    "glob": "^7.2.0",
    "html-minifier": "^4.0.0",
    "luxon": "^2.3.1",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-attrs": "^4.1.3",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-link-attributes": "^4.0.0",
    "markdown-it-prism": "^2.2.3",
    "md5": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rss": "^1.2.2",
    "sass": "^1.49.9"
  }
}
